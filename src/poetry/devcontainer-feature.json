{
    "name": "Poetry",
    "id": "poetry",
    "version": "0.0.1",
    "description": "Custom Poetry installation",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "1.2.2",
                "1.3.2",
                "1.4.2"
            ],
            "default": "1.4.2",
            "description": "Select a version to install"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}
